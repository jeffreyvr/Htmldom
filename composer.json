{
    "name": "jeffreyvr/htmldom",
    "description": "Simple Html Dom Parser for Laravel",
    "keywords": ["laravel", "html parser"],
    "license": "MIT",
    "authors": [
        {
            "name": "yangqi",
            "email": "i@yangqi.me"
        },
        {
            "name": "Jeffrey van Rossum",
            "email": "jeffrey@vanrossum.dev"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Yangqi\\Htmldom\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yangqi\\Htmldom\\HtmldomServiceProvider"
            ]
        }
    },
    "scripts": {
        "sniffer:php8": "phpcs -p ./src --standard=vendor/phpcompatibility/php-compatibility/PHPCompatibility --runtime-set testVersion 8.0"
    },
    "minimum-stability": "dev",
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.1.0",
        "phpcompatibility/php-compatibility": "9.3.5"
    }
}
